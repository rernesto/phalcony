{
    "type": "project",
    "name": "phalcon/phalcony",
    "keywords":  [
        "phalcony",
        "framework",
        "skeleton",
        "boilerplate"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Rafael E. Espinosa Santiesteban <@rernesto>",
            "email": "rernesto.espinosa@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "config/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phalcon/migrations": "^2.2",
        "phpunit/phpunit": "^9.0",
        "phalcon/incubator-test": "^v1.0.0-alpha.1",
        "phalcon/ide-stubs": "^4.1",
        "psr/log": "^1.1"
    },
    "require": {
        "php": "^7.4",
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-phalcon": "^4.0",
        "vlucas/phpdotenv": "^5.4",
        "symfony/var-dumper": "^5.4"
    }
}
